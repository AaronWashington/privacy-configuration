{
    "readme": "https://github.com/duckduckgo/privacy-configuration/apptp/",
    "features": {
        "connectivityChecks": {
            "state": "enabled"
        },
        "ipv6Support": {
            "state": "enabled"
        },
        "privateDnsSupport": {
            "state": "enabled"
        },
        "badHealthMitigation": {
            "state": "enabled",
            "settings": {
                "triggers": {
                    "socketReadExceptionAlerts": {
                        "state": "enabled",
                        "threshold": 20
                    }
                }
            }
        },
        "cpuMonitoring": {
            "state": "enabled"
        }
    },
    "lists": {}
}
        