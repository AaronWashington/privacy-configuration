{
    "version": "2021.6.7",
    "readme": "https://github.com/duckduckgo/privacy-configuration",
    "privacyFeatures": {
        "contentBlocking": {
            "state": "enabled"
        },
        "fingerprintProtections": {
            "state": "enabled"
        },
        "cookieProtections": {
            "state": "enabled",
            "firstPartyTrackerCookiePolicy": {
                "threshold": 86400,
                "maxAge": 86400
            }
        },
        "clickToPlay": {
            "state": "enabled"
        },
        "canvas": {
            "state": "enabled"
        },
        "audio": {
            "state": "enabled"
        },
        "temporaryStorage": {
            "state": "enabled"
        },
        "referer": {
            "state": "enabled"
        },
        "battery": {
            "state": "enabled"
        },
        "screenSize": {
            "state": "enabled"
        },
        "hardware": {
            "state": "enabled"
        },
        "doNotTrack": {
            "state": "enabled"
        },
        "floc": {
            "state": "enabled"
        },
        "gpc": {
            "state": "enabled"
        },
        "autofill": {
            "state": "enabled"
        }
    },
    "allow": {
        "fingerprinting": { }
    }
}
